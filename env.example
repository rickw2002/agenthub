# Database
DATABASE_URL="postgresql://<user>:<password>@<host>:5432/<database>?schema=public"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="change-this-to-a-random-secret-in-production"

# Agent Service (for n8n callbacks)
AGENT_SERVICE_KEY="change-this-to-a-random-secret-key-for-service-callbacks"

# Agent Service Key (public, for local testing only - DO NOT USE IN PRODUCTION)
NEXT_PUBLIC_AGENT_SERVICE_KEY="change-this-to-same-value-as-AGENT_SERVICE_KEY-for-local-testing"

# n8n Webhook URL
N8N_RUN_WEBHOOK_URL="http://localhost:5678/webhook/agenthub/run"

